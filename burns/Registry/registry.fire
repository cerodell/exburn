#
# ----------------------------------------
#  begin fire variables and configuration
# ----------------------------------------
#
# declare fire package and choose which fire scheme
#
#<key>   <package       <associated               <package        <associated 4d scalars>
#         name>          namelist choice>          state vars>
#
package   fire_sfire    ifire==2                    -             state:avg_fuel_frac,bbb,betafl,burnt_area_dt,canhfx,canqfx,dzdxf,dzdyf,fcanhfx,fcanqfx,fgip,fgrnhfx,fgrnqfx,fire_area,fire_smoke,flame_length,fmc_equi,fmc_g,fmc_gc,fmep,fmc_lag,fmoist_lasttime,fmoist_nexttime,fuel_frac,fuel_time,fxlat,fxlong,fz0,grnhfx,grnhfx_fu,grnqfx,grnqfx_fu,iboros,ischap,lfn,lfn_0,lfn_1,lfn_2,lfn_hist,lfn_s0,lfn_s1,lfn_s2,lfn_s3,lfn_time,nfuel_cat,phiwc,psfc_old,q2_old,r_0,rain_old,rh_fire,ros,ros_front,rqvfrten,rthfrten,t2_old,tign_g,uah,uf,vah,vf,zsf

package   fire_sfire1    ifire==1                    -             state:avg_fuel_frac,nfuel_cat,zsf,tign_g,tign_in,fire_hfx,rthfrten,rqvfrten,grnhfx,grnqfx,canhfx,canqfx,dzdxf,dzdyf,lfn,lfn_out,fuel_frac,fire_area,uf,vf,fgrnhfx,fgrnqfx,fcanhfx,fcanhfx,fcanqfx,ros,fxlong,fxlat,fuel_time,bbb,phiwc,phisc,r_0,fgip,ischap,fz0,fwh,unit_fxlong,unit_fxlat,ndwi,ndvi,fmc_g,fmc_gc,fndwi,flineint,flineint2,f_ros0,f_rosx,f_rosy,f_ros,f_int,f_lineint,f_lineint2,f_ros11,f_ros12,f_ros13,f_ros21,f_ros23,f_ros31,f_ros32,f_ros33,wz0,can_top,cuf,cvf,uah,vah

package  fire_fmoist_run   fmoisti_run==1           -             state:fmc_gc,fmep,fmc_equi,fmc_lag,t2_old,q2_old,rain_old,psfc_old,rh_fire
package  fire_fmoist_interp fmoisti_interp==1       -             state:fmc_gc

# level function history support
dimspec   ign   2           constant=1      z    i_lfn_history
state     real  lfn_hist    *i*j        fire   1    Z   i012r     "LFN_HIST"    "level function history" "1"
state     real  lfn_time    {ign}       fire   1    -   i012r     "LFN_TIME"    "level function history time" "s"


# fire variables on fire grid
#
#<Table> <Type> <Sym>       <Dims>   <Use>   <NumTLev> <Stagger> <IO>     <DNAME>             <DESCRIP>     <UNITS>
state    real  nfuel_cat *i*j     fire        1         z     i012r        "NFUEL_CAT"        "fuel data"
state    real     zsf       *i*j     fire        1         z   i012r   "ZSF"      "height of surface above sea level" "m"
state    real     dzdxf     *i*j     fire        1         z   i012r   "DZDXF"    "surface gradient x" "1"
state    real     dzdyf     *i*j     fire        1         z   i012r   "DZDYF"    "surface gradient y" "1"
state    real     tign_g    *i*j     fire        1         z   i012r   "TIGN_G"   "ignition time on ground" "s"
state    real     fire_hfx  *i*j     fire        1         z   i012r   "FIRE_HFX" "observed fire heat flux" "W/m^2"

# fire input variables on atm grid
#
# satellite sensing
state    real     ndwi      ij      fire        1         z   i012r    "NDWI"    "Normalized Difference Water Index" "1"
state    real     ndvi      ij      fire        1         z   i012r    "NDVI"    "Normalized Difference Vegetation Index" "1"
#
# outputs to atm model
state    real     rthfrten  ikj      fire        1         z     r   "RTHFRTEN" "temperature tendency" "K/s"
state    real     rqvfrten  ikj      fire        1         z     r   "RQVFRTEN" "humidity tendency"

# diagnostics only
state    real     avg_fuel_frac  ij       fire        1         z     r   "AVG_FUEL_FRAC" "fuel remaining averaged to atmospheric grid" "1"
state    real     grnhfx         ij       fire        1         z     hr   "GRNHFX"   "heat flux from ground fire" "W/m^2"
state    real     grnqfx         ij       fire        1         z     hr   "GRNQFX"   "moisture flux from ground fire" "W/m^2"
state    real     canhfx         ij       fire        1         z     r   "CANHFX"   "heat flux from crown fire" "W/m^2"
state    real     canqfx         ij       fire        1         z     r   "CANQFX"   "moisture flux from crown fire" "W/m^2"
state    real     uah            ij       fire        1         X     r   "UAH"      "wind at fire_wind_height" "m/s"
state    real     vah            ij       fire        1         Y     r   "VAH"      "wind at fire_wind_height" "m/s"
state    real     grnhfx_fu      ij       fire        1         z     r   "GRNHFX_FU"   "heat flux from ground fire (feedback unsensitive)" "W/m^2"
state    real     grnqfx_fu      ij       fire        1         z     r  "GRNQFX_FU"   "moisture flux from ground fire (feedback unsensitive)" "W/m^2"

# sfire variables on fire grid
# (also using tign_g,zs,z_at_w,dz8w,nfuel_cat,fluxes,zsf)
#
state    real     lfn       *i*j      fire        1         z    r    "LFN" "level function" "1"
state    real     lfn_0     *i*j      fire        1         z    r    "LFN_0" "level function for time integration, step 0" "1"
state    real     lfn_1     *i*j      fire        1         z    r    "LFN_1" "level function for time integration, step 1" "1"
state    real     lfn_2     *i*j      fire        1         z    r    "LFN_2" "level function for time integration, step 2" "1"
state    real     lfn_s0    *i*j      fire        1         z    r    "LFN_S0" "level set sign function from LSM integration" "1"
state    real     lfn_s1    *i*j      fire        1         z    r    "LFN_S1" "level set function for reinitialization integration" "1"
state    real     lfn_s2    *i*j      fire        1         z    r    "LFN_S2" "level set function for reinitialization integration" "1"
state    real     lfn_s3    *i*j      fire        1         z    r    "LFN_S3" "level set function for reinitialization integration" "1"
#
state    real     fuel_frac *i*j      fire        1         z    r    "FUEL_FRAC" "fuel remaining" "1"
state    real     fire_area *i*j      fire        1         z    r    "FIRE_AREA" "fraction of cell area on fire" "1"

# sfire variables on fire grid
# (also using inputs: zs,z_at_w,dz8w,nfuel_cat,zsf)
#
state    real     tign_in   *i*j      fire        1         z  i102r   "TIGN_IN"    "minimal ignition time on ground" "s"
state    real     lfn_out   *i*j      fire        1         z    r "LFN_OUT"       "level function at time step end, for halo" "1"
state    real     fuel_frac_burnt *i*j fire       1         z    r    "FUEL_FRAC_BURNT" "fraction of fuel burnt in timestep" "1"
state    real     uf        *i*j      fire        1         z    r    "UF"        "fire wind" "m/s"
state    real     vf        *i*j      fire        1         z    r    "VF"        "fire wind" "m/s"
state    real     cuf       *i*j      fire        1         z    r    "CUF"       "U-wind at canopy top" "m/s"
state    real     cvf       *i*j      fire        1         z    r    "CVF"       "V-wind at canopy top" "m/s"
state    real     fgrnhfx   *i*j      fire        1         z    r    "FGRNHFX"   "heat flux from ground fire" "W/m^2"
state    real     fgrnqfx   *i*j      fire        1         z    r    "FGRNQFX"   "moisture flux from ground fire" "W/m^2"
state    real     fcanhfx   *i*j      fire        1         z    r    "FCANHFX"   "heat flux from crown fire" "W/m^2"
state    real     fcanqfx   *i*j      fire        1         z    r    "FCANQFX"   "moisture flux from crown fire" "W/m^2"
state    real     ros       *i*j      fire        1         z    r    "ROS" "rate of spread" "m/s"
state    real     burnt_area_dt *i*j      fire        1         z    r    "BURNT_AREA_DT" "fraction of cell area burnt on current dt" "-"
state    real     flame_length  *i*j      fire        1         z    r    "FLAME_LENGTH"     "fire flame length" "m"
state    real     ros_front     *i*j      fire        1         z    r    "ROS_FRONT"     "rate of spread at fire front" "m/s"

# fuel moisture variables
state    real     fmc_g     *i*j      fire        1         z   i012r "FMC_G"     "ground fuel moisture contents" "1"
state    real     fndwi     *i*j      fire        1         z   i012r    "FNDWI"    "Normalized Difference Water Index on fire grid" "1"

# fuel moisture model section
dimspec  num_fmc    -   namelist=nfmc              z      fuel_moisture_classes
dimspec  num_fmep    -   constant=2                z      fuel_moisture_extended_parameters
rconfig  integer  nfmc  namelist,fire  1    5  -   "nfmc" "number of fuel moisture classes"
state    real     fmc_gc  i{num_fmc}j fire      1           z   i012r   "FMC_GC"  "fuel moisture contents by class" "1"
state    real     fmep    i{num_fmep}j fire      1         z    i012r   "FMEP"  "fuel moisture extended model parameters" "1"
state    real     fmc_equi  i{num_fmc}j fire     1         z     r   "FMC_EQUI"  "fuel moisture contents by class equilibrium (diagnostics only)" "1"
state    real     fmc_lag  i{num_fmc}j fire     1         z      r   "FMC_TEND"  "fuel moisture contents by class time lag (diagnostics only)" "h"
state    real     rain_old  ij       fire        1         z     r   "RAIN_OLD" "previous value of accumulated rain" "mm"
state    real     t2_old    ij       fire        1         z     r   "T2_OLD" "previous value of air temperature at 2m" "K"
state    real     q2_old    ij       fire        1         z     r   "Q2_OLD" "previous value of 2m specific humidity" "kg/kg"
state    real     psfc_old  ij       fire        1         z     r   "PSFC_OLD" "previous value of surface pressure" "Pa"
state    real     rh_fire   ij       fire        1         z     r   "RH_FIRE" "relative humidity at the surface" "1"
state    real     fmoist_lasttime -  fire        -         -     r   "FMOIST_LASTTIME" "last time the moisture model was run" "s"
state    real     fmoist_nexttime -  fire        -         -     r   "FMOIST_NEXTTIME" "next time the moisture model will run" "s"
rconfig logical   fmoist_run        namelist,fire  max_domains   .false.       hr   "run moisture model (on the atmospheric grid), output to fmc_gc"
rconfig logical   fmoist_interp     namelist,fire  max_domains   .false.       hr   "interpolate moisture from the model or the input to fuels on the fire grid"
rconfig integer   fmoisti_run       derived        max_domains   0             hr   "run moisture model (on the atmospheric grid), output to fmc_gc"
rconfig integer   fmoisti_interp    derived        max_domains   0             hr   "interpolate moisture from the model or the input to fuels on the fire grid"
rconfig logical   fmoist_only       namelist,fire  max_domains   .false.       hr   "only run moisture model, skip fire"
rconfig integer   fmoist_freq       namelist,fire  max_domains    0       hr   "fmoist_freq"    "frequency to run moisture model 0: use fmoist_dt, k>0: every k timesteps" "1"
rconfig real      fmoist_dt         namelist,fire  max_domains    600     hr   "fmoist_dt  "    "moisture model time step" "s"
rconfig real      fmep_decay_tlag   namelist,fire  1              999999  hr   "fmep_decay_tlag" "time constant of assimilated adjustments of equilibria decay" "1"
halo      HALO_FIRE_MFG dyn_em 24:fmc_g
halo      HALO_FIRE_MAG dyn_em 8:fmc_gc


# constant data arrays
state    real     fxlong    *i*j      fire        1         z   i012r    "FXLONG"    "longitude of midpoints of fire cells" "degrees"
state    real     fxlat     *i*j      fire        1         z   i012r    "FXLAT"     "latitude of midpoints of fire cells" "degrees"
state    real     fuel_time *i*j      fire        1         z    r    "FUEL_TIME" "fuel"
state    real     bbb       *i*j      fire        1         z    r    "BBB"       "fuel"
state    real     betafl    *i*j      fire        1         z    r    "BETAFL"    "fuel"
state    real     phiwc     *i*j      fire        1         z    r    "PHIWC"     "fuel"
state    real     r_0       *i*j      fire        1         z    r    "R_0"       "fuel"
state    real     fgip      *i*j      fire        1         z    r    "FGIP"      "fuel"
state    real     ischap    *i*j      fire        1         z    r    "ISCHAP"    "fuel"
state    real     fz0        *i*j      fire        1         z   i012r    "FZ0"        "roughness length of fire cells" "m"
state    real     iboros     *i*j      fire        1         z    r    "IBOROS"     "fire intensity over rate of spread" "kJ/m^2"

rconfig  integer  kfmc_ndwi         namelist,fire 1              0 r  "KFMC_NDWI" "number of moisture class to update from NDWI, or zero"
rconfig  integer  fndwi_from_ndwi   namelist,fire 1              1 r  "FNDWI_FROM_NDWI" "number of moisture class to update from NDWI, or zero"
halo      HALO_FIRE_NDWI dyn_em 8:ndwi

# diagnostics
# for the actual modeled fire
state    real     flineint  *i*j      fire        1         z    r    "FLINEINT" "fireline intensity" "W/m"
state    real     flineint2 *i*j      fire        1         z    r    "FLINEINT2" "alternative fireline intensity" "J/m/s^2"
# for fire risk rating - independent on any actual fire going on
state    real     f_ros0    *i*j      fire        1         z    r    "F_ROS0" "base rate of spread in all directions" "m/s"
state    real     f_rosx    *i*j      fire        1         z    r    "F_ROSX" "X component of the spread vector driven by wind and slope" "m/s"
state    real     f_rosy    *i*j      fire        1         z    r    "F_ROSY" "Y component of the spread vector driven by wind and slope" "m/s"
state    real     f_ros     *i*j      fire        1         z    r    "F_ROS" "max spread rate in any direction" "m/s"
state    real     f_int     *i*j      fire        1         z    r    "F_INT" "fire reaction intensity for risk rating, without fire" "J/m^2/s"
state    real     f_lineint *i*j      fire        1         z    r    "F_LINEINT" "Byram fireline intensity for risk rating, without fire" "J/m/s"
state    real     f_lineint2 *i*j     fire        1         z    r    "F_LINEINT2" "alternative fireline intensity for risk rating, without fire" "J/m/s^2"
state    real     f_ros11   *i*j      fire        1         z    r    "F_ROS11" "rate of spread in the direction to node (i-2)+1,(j-2)+1"  "m/s"
state    real     f_ros12   *i*j      fire        1         z    r    "F_ROS12" "rate of spread in the direction to node (i-2)+1,(j-2)+2"  "m/s"
state    real     f_ros13   *i*j      fire        1         z    r    "F_ROS13" "rate of spread in the direction to node (i-2)+1,(j-2)+3"  "m/s"
state    real     f_ros21   *i*j      fire        1         z    r    "F_ROS21" "rate of spread in the direction to node (i-2)+2,(j-2)+1"  "m/s"
state    real     f_ros23   *i*j      fire        1         z    r    "F_ROS23" "rate of spread in the direction to node (i-2)+2,(j-2)+3"  "m/s"
state    real     f_ros31   *i*j      fire        1         z    r    "F_ROS31" "rate of spread in the direction to node (i-2)+3,(j-2)+1"  "m/s"
state    real     f_ros32   *i*j      fire        1         z    r    "F_ROS32" "rate of spread in the direction to node (i-2)+3,(j-2)+2"  "m/s"
state    real     f_ros33   *i*j      fire        1         z    r    "F_ROS33" "rate of spread in the direction to node (i-2)+3,(j-2)+3"  "m/s"

# SFIRE constant data arrays
state    real     unit_fxlong -         -         -         -    r    "UNIT_FXLONG" "length of 1 unit in fxlong" "m"
state    real     unit_fxlat  -         -         -         -    r    "UNIT_FXLAT" "length of 1 unit in fxlat" "m"
state    real     phisc     *i*j      fire        1         z    r    "PHISC"     "fuel"
state    real     wz0       *i*j      fire        1         z    r    "WZ0"       "WRF roughness length"
state    real     fwh       *i*j      fire        1         z    r    "FWH"       "fuel fire wind height"
state    real     can_top   *i*j      fire        1         z   i012r   "CAN_TOP"   "Height of tree canopy"


